--- e2fsprogs-1.38/misc/mke2fs.c	2005-07-01 02:00:40.000000000 +0200
+++ e2fsprogs-1.38-livebox/misc/mke2fs.c	2010-03-31 11:45:59.000000000 +0200
@@ -391,6 +391,22 @@ static void progress_init(struct progres
 
 static void progress_update(struct progress_struct *progress, __u32 val)
 {
+	// Calcul du pourcentage de progression (bug 5877)
+	// On ajoute 16% au nombre d'inode traites pour tenir compte du temps de creation 
+	// du journal, non monitore et representant environ 16% du temps...
+	// On memorise la valeur courante pour eviter d'ecrire le fichier tant que l'on a pas atteint
+	// l'unite suivante.  (par exemple pour 600 inode, entre 5% et 6%, 
+	// on aurait 6 ecritures du fichier avec la valeur 5)
+	static int oldPercent=0;
+	int newPercent=((float)val*100) / ((float)progress->max*1.16);
+	if (newPercent%5 == 0 && oldPercent!=newPercent) {
+		oldPercent=newPercent;
+		// On met a jour l'indicateur externe (fichier /tmp/format_progress) tout les 5%
+		FILE* fd = fopen ("/tmp/format_progress", "w");
+		fprintf (fd, "%d\n", newPercent);
+		fclose(fd);
+	}
+	// Fin de l'ajout (bug 5877)
 	if ((progress->format[0] == 0) || progress->skip_progress)
 		return;
 	printf(progress->format, val, progress->max);
